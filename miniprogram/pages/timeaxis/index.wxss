/* pages/timeaxis/index.wxss */

page {
    background-color: #EFEFF4;
}

.my-timeaxis {
    position: relative;
    height: 500px;
    background-color: #EFEFF4;
}

.add-thing {
    position: fixed;
    bottom: 15px;
    left: 50%;
    transform: translateX(-50%)
}

.my-list {
    overflow: auto;
    margin: auto;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}
.bottom {
    position: fixed;
    display: flex;
    justify-content: space-around;
    align-items: center;
    bottom: 0;
    width: 100%;
    height: 80px;
    /* background-color: lightblue; */
}
.my-image {
    width: 40px;
    height: 40px;
}
